TD_mac_tb.run_simd ;
    %load/vec4 v0x1340ae0_0;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1341aa0_0, 4, 2;
    %load/vec4 v0x1340ca0_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1341aa0_0, 4, 2;
    %load/vec4 v0x1340e40_0;
    %store/vec4 v0x1341b80_0, 0, 4;
    %load/vec4 v0x1340f70_0;
    %store/vec4 v0x1341c50_0, 0, 4;
    %load/vec4 v0x1341050_0;
    %pad/s 9;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1341d50_0, 4, 9;
    %load/vec4 v0x1341130_0;
    %ix/load 4, 9, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x1341d50_0, 4, 9;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1341ee0_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x1340ae0_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 5;
    %store/vec4 v0x1340bc0_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x1340ca0_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 5;
    %store/vec4 v0x1340d60_0, 0, 5;
    %load/vec4 v0x1340bc0_0;
    %pad/s 18;
    %load/vec4 v0x1340e40_0;
    %pad/s 18;
    %mul;
    %load/vec4 v0x1341050_0;
    %add;
    %pad/s 9;
    %store/vec4 v0x1341210_0, 0, 9;
    %load/vec4 v0x1340d60_0;
    %pad/s 9;
    %load/vec4 v0x1340f70_0;
    %pad/s 9;
    %mul;
    %load/vec4 v0x1341130_0;
    %add;
    %store/vec4 v0x1341380_0, 0, 9;
    %load/vec4 v0x1340ae0_0;
    %load/vec4 v0x1340ca0_0;
    %load/vec4 v0x1341df0_0;
    %parti/s 9, 0, 2;
    %load/vec4 v0x1341df0_0;
    %parti/s 9, 9, 5;
    %load/vec4 v0x1341df0_0;
    %parti/s 9, 0, 2;
    %load/vec4 v0x1341210_0;
    %cmp/e;
    %flag_get/vec4 6;
    %load/vec4 v0x1341df0_0;
    %parti/s 9, 9, 5;
    %load/vec4 v0x1341380_0;
    %cmp/e;
    %flag_get/vec4 6;
    %and;
    %flag_set/vec4 8;
    %jmp/0 T_0.0, 8;
    %pushi/vec4 1346458451, 0, 32; draw_string_vec4
    %jmp/1 T_0.1, 8;
T_0.0 ; End of true expr.
    %pushi/vec4 1178683724, 0, 32; draw_string_vec4
    %jmp/0 T_0.1, 8;
 ; End of false expr.
    %blend;
T_0.1;
    %vpi_call 2 80 "$display", "a0=%0d a1=%0d b0=%0d b1=%0d c0=%0d c1=%0d |  psum0=%0d psum1=%0d | expected0=%0d expected1=%0d %s", S<4,vec4,u2>, S<3,vec4,u2>, v0x1340e40_0, v0x1340f70_0, v0x1341050_0, v0x1341130_0, S<2,vec4,s9>, S<1,vec4,s9>, v0x1341210_0, v0x1341380_0, S<0,vec4,u32> {5 0 0};
    %end;
