verilator -Wall -j 0 --binary -cc -f filelists/core_tb.f --timing --timescale 1ns/1ps --top-module core_tb --trace -Wno-UNOPTFLAT -Wno-GENUNNAMED -Wno-WIDTHEXPAND -Wno-WIDTHTRUNC -Wno-USERFATAL -Wno-ASCRANGE -Wno-DECLFILENAME -Wno-IMPORTSTAR -Wno-WIDTHCONCAT -Wno-PINCONNECTEMPTY -Wno-UNUSEDSIGNAL
make -C obj_dir/ -f Vcore_tb.mk Vcore_tb
./obj_dir/Vcore_tb
